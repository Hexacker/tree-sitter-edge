{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "edge",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "_node": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "doctype"
        },
        {
          "type": "SYMBOL",
          "name": "element"
        },
        {
          "type": "SYMBOL",
          "name": "directive"
        },
        {
          "type": "SYMBOL",
          "name": "output_expression"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "raw_text"
        }
      ]
    },
    "element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "open_tag"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_node"
              }
            },
            {
              "type": "SYMBOL",
              "name": "close_tag"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "self_closing_tag"
        }
      ]
    },
    "open_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "close_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "</"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "self_closing_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": "/>"
        }
      ]
    },
    "doctype": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<!DOCTYPE"
        },
        {
          "type": "PATTERN",
          "value": "\\s+"
        },
        {
          "type": "STRING",
          "value": "html"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_\\-:]*"
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "attribute_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "quoted_attribute_value"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "attribute_value"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "attribute_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_:][a-zA-Z0-9_:\\-\\.]*"
    },
    "attribute_value": {
      "type": "PATTERN",
      "value": "[^\\s\"'=<>`]+"
    },
    "quoted_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "attribute_content"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "'"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "attribute_content"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "'"
            }
          ]
        }
      ]
    },
    "attribute_content": {
      "type": "PATTERN",
      "value": "[^\"']*"
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "if_directive"
        },
        {
          "type": "SYMBOL",
          "name": "each_directive"
        },
        {
          "type": "SYMBOL",
          "name": "component_directive"
        },
        {
          "type": "SYMBOL",
          "name": "slot_directive"
        },
        {
          "type": "SYMBOL",
          "name": "include_directive"
        },
        {
          "type": "SYMBOL",
          "name": "let_directive"
        },
        {
          "type": "SYMBOL",
          "name": "raw_directive"
        }
      ]
    },
    "if_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@if"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        },
        {
          "type": "SYMBOL",
          "name": "directive_content"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "else_directive"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "@end"
        }
      ]
    },
    "else_directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "@else"
            },
            {
              "type": "SYMBOL",
              "name": "directive_content"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "@elseif"
            },
            {
              "type": "SYMBOL",
              "name": "directive_params"
            },
            {
              "type": "SYMBOL",
              "name": "directive_content"
            }
          ]
        }
      ]
    },
    "each_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@each"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        },
        {
          "type": "SYMBOL",
          "name": "directive_content"
        },
        {
          "type": "STRING",
          "value": "@end"
        }
      ]
    },
    "component_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@component"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        },
        {
          "type": "SYMBOL",
          "name": "directive_content"
        },
        {
          "type": "STRING",
          "value": "@end"
        }
      ]
    },
    "slot_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@slot"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        },
        {
          "type": "SYMBOL",
          "name": "directive_content"
        },
        {
          "type": "STRING",
          "value": "@end"
        }
      ]
    },
    "include_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@include"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        }
      ]
    },
    "let_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@let"
        },
        {
          "type": "SYMBOL",
          "name": "directive_params"
        }
      ]
    },
    "raw_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "directive_params"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "directive_params": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "js_expression"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "directive_content": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_node"
      }
    },
    "output_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{{"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "js_expression"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "}}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{{{"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "js_expression"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "}}}"
            }
          ]
        }
      ]
    },
    "js_expression": {
      "type": "PATTERN",
      "value": "[^){}]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "{{--"
            },
            {
              "type": "PATTERN",
              "value": "[^-]*(-[^-}])*"
            },
            {
              "type": "STRING",
              "value": "--}}"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "<!--"
            },
            {
              "type": "PATTERN",
              "value": "[^-]*(-[^-])*"
            },
            {
              "type": "STRING",
              "value": "-->"
            }
          ]
        }
      ]
    },
    "raw_text": {
      "type": "PATTERN",
      "value": "[^<@{]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}